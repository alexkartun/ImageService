@model ImageWebService.Models.ConfigModel
@{
    ViewBag.Title = "Config";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Service Config</h2>

<p>
    Output Directory:
    @Model.OutputDir
</p>
<br />
<p>
    Source Name:
    @Model.SourceName
</p>
<br />
<p>
    Log Name:
    @Model.LogName
</p>
<br />
<p>
    Thumbnail Size:
    @Model.ThumbnailSize
</p>
<br />

@foreach (var item in Model.DirectoryHandlers)
{
    <a href="@Url.Action("RemoveConfirm", "Home", new {handler = @item})">@item</a>
    <br />
}