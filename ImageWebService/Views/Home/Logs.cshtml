@model ImageWebService.Models.Logs
@{
    ViewBag.Title = "Logs";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Logs</h2>
Type:
@using (Html.BeginForm("LogsByType", "Home"))
{
    @Html.TextBoxFor(Model => Model.Type)
    <input type="submit" value="Type" class="btn-default" />
}

<table class="table">
    <tr>
        <th>
            Message
        </th>
        <th>
            Type
        </th>
    </tr>

    @foreach (var item in Model.ServiceLogs)
    {
        <tr>
            <td>
                @item.Message
            </td>
            <td>
                @item.Status.ToString()
            </td>
        </tr>
    }
</table>


